/* 水平间距 */
/* 水平间距 */
.all {
  padding-bottom: 136rpx;
}
.more-container {
  height: auto;
  width: 100%;
  min-height: 100vh;
  background-color: #F8F8F8;
  padding-bottom: env(safe-area-inset-bottom);
}
.more-container .placeholder {
  width: 100%;
  background-color: #F8F8F8;
  position: fixed;
  top: 0;
  z-index: 9999;
}
.more-container .custom-bar {
  width: 100%;
  height: 104rpx;
  box-shadow: 0 6rpx 12rpx 2rpx rgba(0, 0, 0, 0.04);
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 15rpx;
  position: relative;
  background-color: #fff;
  color: #1E1E20;
  font-size: 32rpx;
  font-weight: 400;
  position: fixed;
  top: 0;
  z-index: 9998;
}
.more-container .custom-bar .back {
  width: 39rpx;
  height: 32rpx;
  position: absolute;
  left: 42rpx;
}
.more-container .list-item-container {
  width: 100%;
  height: auto;
  padding: 40rpx 28rpx 40rpx 30rpx;
  background-color: #fff;
  margin-bottom: 16rpx;
  z-index: 1;
}

